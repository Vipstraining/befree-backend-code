{
  "name": "food-catalog-backend",
  "version": "1.0.0",
  "description": "Comprehensive food catalog backend with nutritional analysis using Claude AI",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "start:staging": "NODE_ENV=staging node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "dev:debug": "NODE_ENV=development DEBUG=* nodemon server.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "docs:html": "node scripts/generate-pdf.js",
    "docs:pdf": "node scripts/generate-pdf.js",
    "docs:open": "node scripts/open-docs.js",
    "seed": "NODE_ENV=development node scripts/seed.js",
    "seed:prod": "NODE_ENV=production node scripts/seed.js",
    "build": "echo 'No build step required for Node.js'",
    "deploy:staging": "NODE_ENV=staging npm start",
    "deploy:prod": "NODE_ENV=production npm start"
