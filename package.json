  "keywords": ["food", "nutrition", "claude", "ai", "catalog"],
  "author": "Befree",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "@anthropic-ai/sdk": "^0.9.1",
    "axios": "^1.6.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "qrcode": "^1.5.3",
    "node-cron": "^3.0.3"
  },
